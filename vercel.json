{
  "version": 2,
  "builds": [
    {
      "src": "daily-david-modern/server/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "daily-david-modern/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/daily-david-modern/server/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/daily-david-modern/index.html"
    }
  ]
}