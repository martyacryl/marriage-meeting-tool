{
  "version": 2,
  "builds": [
    {
      "src": "daily-david-modern/server/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "daily-david-modern/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
